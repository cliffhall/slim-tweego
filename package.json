{
  "name": "slim-tweego",
  "version": "1.0.0",
  "description": "A streamlined tweego project skeleton.",
  "main": "./dist/index.html",
  "config": {
    "format": "sugarcube-2"
  },
  "scripts": {
    "build-once": "gulp build && tweego -f $npm_package_config_format -o dist/index.html --head=src/head-content.html project",
    "watch:gulp": "gulp default",
    "watch:twee": "tweego -f $npm_package_config_format -w -o dist/index.html --head=src/head-content.html project"
  },
  "keywords": [
    "twine",
    "tweego",
    "twee"
  ],
  "contributors": [
    {
      "name": "Cliff Hall",
      "url": "https://github.com/cliffhall",
      "email": "cliff@futurescale.com"
    },
    {
      "name": "Chapel",
      "url": "https://github.com/ChapelR",
      "email": "chapel@twinelab.net"
    }
  ],
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "babel-preset-env": "^1.7.0",
    "del": "^5.1.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.1",
    "gulp-csslint": "^1.0.1",
    "gulp-jshint": "^2.1.0",
    "gulp-noop": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "jshint": "^2.10.2",
    "node-sass": "^4.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cliffhall/slim-tweego.git"
  },
  "bugs": {
    "url": "https://github.com/cliffhall/slim-tweego/issues"
  },
  "homepage": "https://github.com/cliffhall/slim-tweego#readme"
}
